        <access>package_private</access>
        <active>true</active>
        <api_name>x_156327_shark.CatalogItemUtilsAjax</api_name>
        <caller_access/>
        <client_callable>true</client_callable>
        <description>captura email do usuario </description>
        <name>CatalogItemUtilsAjax</name>
        <script><![CDATA[var CatalogItemUtilsAjax = Class.create();
CatalogItemUtilsAjax.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	findEmail: function(){
		var userSysId = this.getParameter('sysparm_user_sys_id');
		var grUser = new GlideRecord('sys_user');
		grUser.addQuery('sys_id', userSysId);
		grUser.setLimit(1);
		grUser.query();
		
		if(grUser.next()){
			return grUser.getValue('email');
		}
	},
    type: 'CatalogItemUtilsAjax'
});]]></script>
